{
	"name": "tourist-hub-api",
	"version": "1.0.0",
	"description": "Tourist Hub API backend for tour management and collaboration",
	"main": "dist/server.js",
	"scripts": {
		"build": "tsc",
		"start": "node dist/server.js",
		"dev": "nodemon --exec \"ts-node --transpile-only\" src/server.ts",
		"dev:watch": "nodemon --exec ts-node src/server.ts",
		"clean": "rimraf dist",
		"prebuild": "npm run clean",
		"test": "vitest --run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"lint": "echo \"Linting not configured yet\"",
		"type-check": "tsc --noEmit",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:migrate": "prisma migrate dev",
		"vercel-build": "npm run db:generate && npm run build",
		"postinstall": "prisma generate",
		"db:migrate:deploy": "prisma migrate deploy",
		"db:reset": "prisma migrate reset",
		"db:seed": "ts-node prisma/seed.ts",
		"db:studio": "prisma studio"
	},
	"keywords": [
		"tourism",
		"api",
		"backend",
		"tour-management",
		"express",
		"typescript"
	],
	"author": "",
	"license": "ISC",
	"type": "commonjs",
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=8.0.0"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.859.0",
		"@aws-sdk/s3-request-presigner": "^3.859.0",
		"@prisma/client": "^6.13.0",
		"@types/bcrypt": "^6.0.0",
		"@types/express": "^4.17.23",
		"@types/joi": "^17.2.2",
		"@types/js-yaml": "^4.0.9",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/multer": "^2.0.0",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"@types/uuid": "^10.0.0",
		"@vitest/ui": "^3.2.4",
		"bcrypt": "^6.0.0",
		"cors": "^2.8.5",
		"dotenv": "^17.2.1",
		"express": "^4.21.2",
		"express-rate-limit": "^8.0.1",
		"helmet": "^8.1.0",
		"joi": "^17.13.3",
		"js-yaml": "^4.1.0",
		"jsonwebtoken": "^9.0.2",
		"multer": "^2.0.2",
		"pg": "^8.16.3",
		"prisma": "^6.13.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"uuid": "^11.1.0",
		"vitest": "^3.2.4",
		"winston": "^3.17.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.19",
		"@types/node": "^24.1.0",
		"@types/pg": "^8.15.5",
		"@types/supertest": "^6.0.3",
		"@types/winston": "^2.4.4",
		"@vercel/node": "^5.3.11",
		"@vitest/coverage-v8": "^3.2.4",
		"nodemon": "^3.1.10",
		"rimraf": "^6.0.1",
		"supertest": "^7.1.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	}
}
